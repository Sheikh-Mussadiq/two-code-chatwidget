{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist-widget"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/two-code-chat-widget.umd.js",
      "dest": "/two-code-chat-widget.umd.js"
    },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
